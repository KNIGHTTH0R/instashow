{% extends "partials/layout_photos.html.tpl" %}{% block title %}Photo{% endblock %}{% block name %}Photo :: {{ media.id }}{% endblock %}{% block content %}    <div class="image">        {% if media.link %}            <a href="{{ media.link }}">                <img src="{{ media.images.standard_resolution.url }}" height="480" width="480" />            </a>        {% else %}            <img src="{{ media.images.standard_resolution.url }}" height="480" width="480" />        {% endif %}    </div>{% endblock %}
