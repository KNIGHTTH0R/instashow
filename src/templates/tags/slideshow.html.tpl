{% extends "partials/layout_complete.html.tpl" %}{% block title %}Tags{% endblock %}{% block name %}Tags :: {{ tag }}{% endblock %}{% block content %}    <div class="instashow" data-url="{{ url_for('latest_tag_json', tag = tag) }}" data-timeout="{{ timeout }}">        <div class="initial">            <div class="page-container">                <h1>Daniela  + Jo√£o</h1>                <h2>07.09.2013</h2>                <div class="instagram"></div>                <p>                    Publica as tuas fotos no instagram <br />                    com a tag <strong>#{{ tag }}</strong>.                </p>                <div class="icon"></div>            </div>        </div>        {% for _media in media %}            <div class="photo">                <img src="{{ _media.images.standard_resolution.url }}" />            </div>        {% endfor %}    </div>{% endblock %}
