{% extends "partials/layout_complete.html.tpl" %}{% block title %}Tags{% endblock %}{% block name %}Tags :: {{ tag }}{% endblock %}{% block content %}    <div class="instashow" data-url="{{ url_for('latest_tag_json', tag = tag) }}" data-timeout="{{ timeout }}">        <div class="initial">            <div class="page-container">                <h1>{{ title }}</h1>                <h2>{{ sub_title }}</h2>                <div class="instagram"></div>                <p>                    Publica as tuas fotos no instagram <br />                    com a tag <strong>#{{ tag }}</strong>.                </p>                <div class="icon"></div>            </div>        </div>        {% for _media in media %}            <div class="item">                {% if _media.type == "video" %}                    <video src="{{ _media.videos.standard_resolution.url }}" loop="1" />                {% else %}                    <img src="{{ _media.images.standard_resolution.url }}" />                {% endif %}            </div>        {% endfor %}    </div>{% endblock %}
